        "@types/express": "^4.17.1",
        "apicache": "^1.5.2",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "moment": "^2.29.2",
        "morgan": "^1.9.1",
        "node-fetch": "^2.6.0",
        "node-schedule": "^1.3.2",
        "ts-node-dev": "^1.0.0-pre.43",
        "typescript": "^3.6.3"
    }
